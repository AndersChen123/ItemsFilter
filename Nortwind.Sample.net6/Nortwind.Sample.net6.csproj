		<Resource Include="..\Northwind.NET.Sample\Resources\EmpID5.gif" Link="Resources\EmpID5.gif" />
		<Resource Include="..\Northwind.NET.Sample\Resources\EmpID6.gif" Link="Resources\EmpID6.gif" />
		<Resource Include="..\Northwind.NET.Sample\Resources\EmpID7.gif" Link="Resources\EmpID7.gif" />
		<Resource Include="..\Northwind.NET.Sample\Resources\EmpID8.gif" Link="Resources\EmpID8.gif" />
		<Resource Include="..\Northwind.NET.Sample\Resources\EmpID9.gif" Link="Resources\EmpID9.gif" />
		<Resource Include="..\Northwind.NET.Sample\Resources\StartupNorthwindLogo.jpg" Link="Resources\StartupNorthwindLogo.jpg" />
		<Resource Include="..\Northwind.NET.Sample\SampleData\CategoriesFilter\CategoriesFilter_Files\image01.png" Link="SampleData\CategoriesFilter\CategoriesFilter_Files\image01.png" />
		<Resource Include="..\Northwind.NET.Sample\SampleData\CategoriesFilter\CategoriesFilter_Files\image02.png" Link="SampleData\CategoriesFilter\CategoriesFilter_Files\image02.png" />
