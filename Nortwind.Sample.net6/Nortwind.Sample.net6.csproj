<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net6.0-windows</TargetFramework>
		<Nullable>enable</Nullable>
		<UseWPF>true</UseWPF>
		<RootNamespace>Northwind.NET.Sample</RootNamespace>
		<ApplicationIcon>ItemsFilter.ico</ApplicationIcon>
		<AssemblyName>$(MSBuildProjectName)</AssemblyName>
	</PropertyGroup>

	<ItemGroup>
		<Page Remove="SampleData\Order.xaml" />
	</ItemGroup>

	<ItemGroup>
		<Compile Include="..\Northwind.NET.Sample\SampleData\CategoriesCollection.cs" Link="SampleData\CategoriesCollection.cs" />
		<Compile Include="..\Northwind.NET.Sample\SampleData\Category.cs" Link="SampleData\Category.cs" />
		<Compile Include="..\Northwind.NET.Sample\SampleData\Customers.cs" Link="SampleData\Customers.cs" />
		<Compile Include="..\Northwind.NET.Sample\SampleData\Employes.cs" Link="SampleData\Employes.cs" />
		<Compile Include="..\Northwind.NET.Sample\SampleData\OrderDetails.cs" Link="SampleData\OrderDetails.cs" />
		<Compile Include="..\Northwind.NET.Sample\SampleData\OrderDetailsCollection.cs" Link="SampleData\OrderDetailsCollection.cs" />
		<Compile Include="..\Northwind.NET.Sample\SampleData\Orders.cs" Link="SampleData\Orders.cs" />
		<Compile Include="..\Northwind.NET.Sample\SampleData\Products.cs" Link="SampleData\Products.cs" />
		<Compile Include="..\Northwind.NET.Sample\SampleData\Suppliers.cs" Link="SampleData\Suppliers.cs">
			<Generator></Generator>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<!--<Content Include="..\Northwind.NET.Sample\Fonts\vijaya.ttf" Link="Fonts\vijaya.ttf" />
    <Content Include="..\Northwind.NET.Sample\Fonts\vijayab.ttf" Link="Fonts\vijayab.ttf" />-->
		<Resource Include="..\Northwind.NET.Sample\Resources\EmpID1.gif" Link="Resources\EmpID1.gif" />
		<Resource Include="..\Northwind.NET.Sample\Resources\EmpID2.gif" Link="Resources\EmpID2.gif" />
